<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Config 3.1//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mafia.srv.aqi.dao.master.AirQualityIndexMapper">

    <insert id="insertBatch" parameterType="list">
        INSERT INTO air_quality_index(
            geo_city,time,aqi,date,create_time)
        VALUES
        <foreach collection='list' item='item' index='index' separator=','>
            (
                #{item.geoCity},#{item.time},#{item.aqi},#{item.date},now()
            )
        </foreach>
    </insert>

</mapper>